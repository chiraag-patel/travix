            <a>carrier</a>
            <b>airline</b>
        </field>
        <field>
            <a>basePrice</a>
            <b>fare</b>
        </field>
        <field>
            <a>departureAirportName</a>
            <b>departureAirportCode</b>
        </field>
        <field>
            <a>arrivalAirportName</a>
            <b>destinationAirportCode</b>
        </field>
        <field>
            <a>outboundDateTime</a>
            <b>departureDate</b>
        </field>
        <field>
            <a>inboundDateTime</a>
            <b>arrivalDate</b>
        </field>
    </mapping>
